<!--pages/login/loginView/loginView.wxml-->

<image class='image' src='/images/img_weixinmini_logo.jpeg'></image>

<view class='title'>沃爱生活</view>

<button class='wechat-login' type='primary' open-type='getPhoneNumber' bindgetphonenumber="getPhoneNumber">
  <image src='/images/btn_login_view_weixin.png'></image>
  <text>微信一键登录/注册</text>
</button>

<button class='phone-login' type='default' bindtap='phoneLogin'>
  <image src='/images/btn_login_view_phone.png'></image>
  <text>手机号登录/注册</text>
</button>

